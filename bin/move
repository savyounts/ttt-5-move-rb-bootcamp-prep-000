#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
prints "Welcome to Tic Tac Toe!"

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Where would you like to go?"
input = gets.strip

input_to_index(input)
index = gets.strip

move(board, index, "X")

display_board(board)
